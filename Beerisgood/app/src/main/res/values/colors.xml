<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Set theme colors from https://material.io/guidelines/style/color.html#color-color-palette-->

    <!-- colorPrimary is used for the default action bar background -->
    <item name="colorPrimary" type="color">#795548</item>
    <!-- colorPrimaryDark is used for the status bar -->
    <item name="colorPrimaryDark" type="color">#6D4C41</item>

    <!-- colorAccent is used as the default value for colorControlActivated which is used to tint widgets -->
    <item name="colorAccent" type="color">#FFB300</item>
    <item name="colorAccentDark" type="color">#FFA000</item>

    <!-- You can also set colorControlNormal, colorControlActivated, colorControlHighlight and colorSwitchThumbNormal. -->
    <item name="colorControlHighlight" type="color">#fffea50b</item>
    <item name="colorControlActivated" type="color">@color/colorAccent</item>

    <!-- Other colors -->
    <item name="red" type="color">#FF0000</item>
    <item name="green" type="color">#00FF00</item>
    <item name="darkgreen" type="color">#388E3C</item>
    <item name="blue" type="color">#0000ff</item>
    <item name="white" type="color">#FFFFFF</item>
    <item name="black" type="color">#212121</item>
    <item name="gray" type="color">#757575</item>
    <item name="vltgray" type="color">#EEEEEE</item>
    <item name="ltgray" type="color">#FFCCCCCC</item>
    <item name="turquoise" type="color">#009688</item>

    <item name="halfTransparent" type="color">#fdffffff</item>

    <!-- MISC -->
    <color name="unpressed_color">#ffffff</color>
    <color name="pressed_color">#2195f2</color>
</resources>
